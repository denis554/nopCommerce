                onclick="javascript:WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions(&quot;ctl00$cph1$ctrlCategoryAdd$SaveButton&quot;, &quot;&quot;, true, &quot;&quot;, &quot;&quot;, false, false))"
                value="Save" name="ctl00$cph1$ctrlCategoryAdd$SaveButton">
            <input type="submit" class="adminButtonBlue" id="ctl00_cph1_ctrlCategoryAdd_SaveAndStayButton"
                onclick="javascript:WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions(&quot;ctl00$cph1$ctrlCategoryAdd$SaveAndStayButton&quot;, &quot;&quot;, true, &quot;&quot;, &quot;&quot;, false, false))"
                value="Save and Continue Edit" name="ctl00$cph1$ctrlCategoryAdd$SaveAndStayButton">*@
        </div>
    </div>
    <table class="adminContent">
