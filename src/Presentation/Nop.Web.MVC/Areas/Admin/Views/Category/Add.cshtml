@using Nop.Web.Framework;
@model Nop.Web.MVC.Areas.Admin.Models.CategoryModel
@{
    ViewBag.Title = "ViewPage1";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@using (Html.BeginForm())
{
    <div class="section-header">
