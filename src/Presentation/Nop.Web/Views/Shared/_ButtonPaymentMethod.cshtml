@model ShoppingCartModel
@if (Model.ButtonPaymentMethodViewComponentNames != null && Model.ButtonPaymentMethodViewComponentNames.Count > 0)
{
    for (int i = 0; i < Model.ButtonPaymentMethodViewComponentNames.Count; i++)
    {
        @await Component.InvokeAsync(Model.ButtonPaymentMethodViewComponentNames[i], Model.ButtonPaymentMethodViewComponentArguments[i])
    }
}