@{
    Layout = "~/Views/Shared/_Root.cshtml";

    //title
    Html.AddTitleParts(T("PageTitle.PageNotFound").Text);
}
<div class="page not-found-page">
    <div class="page-title">
        <h1>@T("PageTitle.PageNotFound")</h1>
    </div>
    <div class="clear">
    </div>
    <div class="page-body">
        <div>
            <strong>The page you requested was not found, and we have a fine guess why.</strong>
            <ul>
                <li>If you typed the URL directly, please make sure the spelling is correct.</li>
                <li>The page no longer exists. In this case, we profusely apologize for the inconvenience and for any damage this may cause.</li>
            </ul>
        </div>
        <p>
            Please go to our home page and try again. 
        </p>
        <p>
            Thank you you for your patience. 
        </p>
    </div>
</div>

