@model EditForumPostModel
@{   
    Layout = "_ColumnsOne";

    Html.AddTitleParts(T("Forum.PageTitle.PostNew").Text);
}
@await Component.InvokeAsync("Widget", new { widgetZone = "boards_postcreate_before", additionalData = Model.Id })
@await Html.PartialAsync("_CreateUpdatePost", Model)
@await Component.InvokeAsync("Widget", new { widgetZone = "boards_postcreate_after", additionalData = Model.Id })
