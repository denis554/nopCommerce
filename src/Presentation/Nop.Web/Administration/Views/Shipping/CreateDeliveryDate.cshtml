@model DeliveryDateModel

@{
    //page title
    ViewBag.Title = T("Admin.Configuration.Shipping.DeliveryDates.AddNew").Text;
    //active menu item (system name)
    Html.SetActiveMenuItemSystemName("Delivery dates");
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="content-header clearfix">
        <h1 class="pull-left">
            @T("Admin.Configuration.Shipping.DeliveryDates.AddNew") 
            <small>@Html.ActionLink("(" + T("Admin.Configuration.Shipping.DeliveryDates.BackToList") + ")", "DeliveryDates")</small>
        </h1>
        <div class="pull-right">
            <input type="submit" name="save" class="btn bg-blue" value="@T("Admin.Common.Save")" />
            <input type="submit" name="save-continue" class="btn bg-blue" value="@T("Admin.Common.SaveContinue")" />
        </div>
    </div>
    @Html.Partial("_CreateOrUpdateDeliveryDate", Model)
}